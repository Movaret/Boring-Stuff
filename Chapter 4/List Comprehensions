# List Comprehensions

# Simple one
print([x**2 for x in range(10)])

# With even numbers only
print([x for x in range(20) if x%2==0])

# Expression as a func argument
def some_function(y):
    return (y + 5) / 2

print([some_function(x) for x in range(10)])

#Dict Comprehensions
dict = {num: num**2 for num in range(5)}

#Set Comprehension
set = {x for x in range(10)}

#Simple list unpacking
lst = [1, 2, 3 ]
x, y, z = lst
print(x, y, z)

#Count most common - 1
from collections import Counter
print(Counter('abracadabra').most_common(3))


#Count most common - 2
from collections import Counter
lst = [2, 2, 2, 5, 5, 6, 7, 6, 9, 2, 4, 8, 5]
print(Counter(lst).most_common(1))
